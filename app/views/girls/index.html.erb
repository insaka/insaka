<div class="container">
    <% @girls.each do |girl| %>
      <div class="row">
        <div class="col-xs-12">
          <h1><a href="/girls/<%= girl.id %>"><%= girl.first_name %> <%= girl.last_name %></a></h1>
          <img src="<%= girl.image %>" / >
          <p>"<%= girl.quote %>"</p>
          <p><%= girl.description %></p>
        </div>
    </div>
  <% end %>
</div>